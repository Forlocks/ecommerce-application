{
  "name": "fun-chat",
  "version": "1.0.0",
  "description": "fun-chat task",
  "scripts": {
    "test": "jest",
    "prod-build": "webpack --env prod",
    "dev-build": "webpack --env dev",
    "dev": "webpack serve --env dev",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier \"src/**/*.ts\" --check",
    "prettier:format": "prettier \"src/**/*.ts\" --write",
    "prepare": "husky && echo npm run lint-staged > .husky/pre-commit && echo npm run validate-branch-name > .husky/pre-push",
    "lint-staged": "lint-staged",
    "validate-branch-name": "validate-branch-name"
  },
  "author": "Code Keepers",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint \"src/**/*.ts\" --fix",
      "prettier \"src/**/*.ts\" --write"
    ]
  },
  "validate-branch-name": {
    "pattern": "^(init|abichores|refactor|docs|feat|fix).*$",
    "errorMsg": "You can't push in this branch"
  }
}
